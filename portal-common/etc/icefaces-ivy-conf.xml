<ivy-module version="2.0"
   xmlns:m="http://ant.apache.org/ivy/maven"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">

   <info organisation="org.eclipse.stardust.ui.web" module="icefaces-impl" revision="${icefaces.version}"/>

   <configurations>
      <conf name="icefaces" description="Config for IceFaces Jars (Community Edition)"/>
   </configurations>
   
   <publications/>
   
   <dependencies>
      <dependency org="org.icefaces" name="icefaces" rev="${icefaces.version}" conf="icefaces->default">
         <exclude org="javax.faces" module="jsf-api" />
         <exclude org="javax.faces" module="jsf-impl" />
         <exclude org="commons-logging" module="commons-logging-api"/>
         <exclude org="commons-logging" module="commons-logging"/>
         <exclude org="commons-beanutils" module="commons-beanutils"/>
         <exclude org="backport-util-concurrent" module="backport-util-concurrent"/>
         <exclude org="com.sun.xml.fastinfoset" module="FastInfoset"/>
      </dependency>
      <!-- force correct revision of commons-logging -->
      <dependency org="commons-logging" name="commons-logging" rev="${commons-logging.version}" conf="icefaces->default">
         <exclude org="logkit" module="logkit" />
         <exclude org="avalon-framework" module="avalon-framework" />
         <exclude org="log4j" module="log4j" />
      </dependency>
      <dependency org="org.icefaces" name="icefaces-comps" rev="${icefaces-comps.version}" conf="icefaces->default">
         <exclude org="org.icefaces" module="icefaces" />
      </dependency>
      <dependency org="org.icefaces" name="icefaces-facelets" rev="${icefaces-facelets.version}" conf="icefaces->default">
         <exclude org="org.icefaces" module="icefaces" />
         <exclude org="javax.el" module="el-api" />
      </dependency>
   </dependencies>
</ivy-module>
