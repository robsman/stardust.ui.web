<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:ippui="http://infinity.com/bpm/portal/common/uiDefinition"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
http://infinity.com/bpm/portal/common/uiDefinition http://infinity.com/bpm/portal/common/uiDefinition.xsd">
	<ippui:perspectiveExtension
		targetPerspective="ippBccPerspective" id="benchmarkPerspective">
		<ippui:launchpadExtension after="myReportsViewPanel">
			<ippui:launchPanel name="benchmarkLaunchPanel"
				include="/plugins/benchmark/launchpanel/benchmarkLaunchPanel.xhtml" />
		</ippui:launchpadExtension>
		<ippui:viewsExtension>
			<ippui:view name="benchmarkView"
				include="/plugins/benchmark/views/benchmarkViewFrameAdapter.xhtml"
				controller="benchmarkView" />
			<ippui:view name="trafficLightViewNew"
				include="/plugins/benchmark/views/trafficLightViewFrameAdapter.xhtml"
				controller="trafficLightViewNew" />
			<ippui:view name="ganttChartViewNew"
				include="/plugins/benchmark/views/ganttChartViewFrameAdapter.xhtml"
				controller="ganttChartViewNew" />
		</ippui:viewsExtension>
	</ippui:perspectiveExtension>
</beans>
